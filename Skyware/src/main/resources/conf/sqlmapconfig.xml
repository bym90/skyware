<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true"/>
		<setting name="useGeneratedKeys" value="true"/>
		<setting name="defaultExecutorType" value="REUSE"/>
	</settings>
	
	<typeAliases>	
<!-- 		<typeAlias alias="test" type="com.scf.skyware.test.domain.Test"/> -->
		<typeAlias alias="code" type="com.scf.skyware.manage.domain.Code"/>
		<typeAlias alias="notice" type="com.scf.skyware.notice.domain.Notice"/>
		<typeAlias alias="user" type="com.scf.skyware.main.domain.User"/>
		<typeAlias alias="auth" type="com.scf.skyware.manage.domain.AdminAuth"/>
		<typeAlias alias="schedule" type="com.scf.skyware.mobile.domain.Schedule"/>
		<typeAlias alias="notice_m" type="com.scf.skyware.mobile.domain.NoticeM"/>
	</typeAliases>
	
	<mappers>
<!-- 		<mapper resource="mybatis/test.xml"/> -->
		<mapper resource="mybatis/code.xml"/>
		<mapper resource="mybatis/notice.xml"/>
		<mapper resource="mybatis/user.xml"/>
		<mapper resource="mybatis/auth.xml"/>
		<mapper resource="mybatis/schedule.xml"/>
	</mappers>
</configuration>	